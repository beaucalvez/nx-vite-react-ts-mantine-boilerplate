{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "apps/main/dist",
  "installCommand": "npm install",
  "framework": "vite",
  "buildCache": {
    "disabled": false
  },
  // Trigger redeploy
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 