{
  "name": "nx-vite-react-ts-mantine-boilerplate",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "init": "npm run clean:all && npm install",
    "dev": "nx serve main",
    "dev:storybook": "nx storybook ui-kit",
    "build:app": "nx build main",
    "build:storybook": "nx storybook:build ui-kit",
    "serve:app": "nx preview main",
    "serve:storybook": "nx storybook:serve ui-kit",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "lint:type-check": "nx run-many --target=type-check --all",
    "lint:stylelint": "nx run-many --target=stylelint --all --fix",
    "test": "VITEST=true nx run-many --target=test --all",
    "test:e2e:storybook": "VITEST=true nx e2e ui-kit",
    "clean:node-modules": "rm -rf node_modules && rm -rf packages/*/node_modules",
    "clean:dist": "rm -rf packages/*/build && rm -rf packages/*/dist",
    "clean:coverage": "rm -rf packages/*/coverage",
    "clean:all": "npm run clean:node-modules && npm run clean:dist && npm run clean:coverage",
    "upgrade:nx": "nx migrate latest && npm install && nx migrate --run-migrations --if-exists && rm -rf migrations.json",
    "upgrade:storybook": "storybook upgrade",
    "prepare": "husky"
  },
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "nx affected --target=lint --fix=true --uncommitted=true",
      "nx affected --target=stylelint --fix=true --uncommitted=true"
    ]
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "@nx/cypress": "20.0.3",
    "@nx/devkit": "20.0.3",
    "@nx/eslint-plugin": "20.0.3",
    "@nx/js": "20.0.3",
    "@nx/node": "20.0.3",
    "@nx/react": "20.0.3",
    "@nx/storybook": "20.0.3",
    "@nx/web": "20.0.3",
    "@nx/workspace": "20.0.3",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "8.3.6",
    "@storybook/core-server": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/react-vite": "^8.3.6",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "7.9.0",
    "@vitejs/plugin-react-swc": "3.6.0",
    "@vitest/coverage-v8": "1.6.0",
    "@vitest/ui": "1.6.0",
    "cypress": "13.15.0",
    "esbuild": "0.21.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-cypress": "3.2.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-nimbus-clean": "2.0.0",
    "eslint-plugin-perfectionist": "3.9.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "eslint-plugin-react-refresh": "0.4.14",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-sonarjs": "2.0.4",
    "eslint-plugin-storybook": "0.10.1",
    "eslint-plugin-testing-library": "6.4.0",
    "eslint-plugin-unicorn": "56.0.0",
    "gh-pages": "6.3.0",
    "husky": "9.1.6",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "nx": "20.0.3",
    "nx-stylelint": "18.0.0",
    "postcss": "8.4.38",
    "postcss-preset-mantine": "1.15.0",
    "postcss-simple-vars": "7.0.1",
    "postcss-syntax": "0.36.2",
    "prettier": "3.3.3",
    "rollup-plugin-analyzer": "4.0.0",
    "rollup-plugin-visualizer": "5.14.0",
    "storybook": "8.3.6",
    "stylelint": "16.5.0",
    "stylelint-config-css-modules": "4.4.0",
    "stylelint-config-standard": "36.0.0",
    "tslib": "2.6.2",
    "vite": "5.4.10",
    "vite-dts": "1.0.4",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  },
  "dependencies": {
    "@mantine/core": "7.13.3",
    "@mantine/form": "7.13.3",
    "@mantine/hooks": "7.13.3",
    "@tabler/icons-react": "3.3.0",
    "clsx": "2.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "typescript": "5.5.4"
  }
}
